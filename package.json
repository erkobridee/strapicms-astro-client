{
  "name": "strapicms-astro-client",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "unset:git-hooks-path": "git config --unset core.hooksPath",
    "simple-git-hooks": "npx simple-git-hooks",
    "postinstall": "is-ci || npm run simple-git-hooks",
    "prettier": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\"",
    "eslint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "lint": "run-s prettier eslint",
    "dev": "astro dev --host",
    "start": "astro dev --host",
    "build": "astro check && astro build",
    "preview": "astro preview --host",
    "sync": "astro sync",
    "astro": "astro",
    "gh-pages-build": "cross-env GITHUB_PAGES=true npm run build",
    "gh-pages-preview": "cross-env GITHUB_PAGES=true GITHUB_PAGES_PREVIEW=true npm run preview",
    "gh-pages-deploy": "gh-pages -d dist",
    "deploy": "run-s gh-pages-build gh-pages-deploy"
  },
  "dependencies": {
    "astro": "5.17.2"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/mdx": "4.3.13",
    "@astrojs/ts-plugin": "1.10.6",
    "@strapi/client": "1.6.0",
    "@tailwindcss/forms": "0.5.11",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@types/node": "24.10.12",
    "@typescript-eslint/eslint-plugin": "8.55.0",
    "@typescript-eslint/parser": "8.55.0",
    "astro-remote": "0.3.4",
    "autoprefixer": "10.4.24",
    "clsx": "2.1.1",
    "cross-env": "10.1.0",
    "eslint": "9.39.2",
    "eslint-plugin-astro": "1.6.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "gh-pages": "6.3.0",
    "globals": "17.3.0",
    "is-ci": "4.1.0",
    "lint-staged": "16.2.7",
    "npm-run-all2": "8.0.4",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "simple-git-hooks": "2.13.1",
    "tailwind-merge": "3.4.1",
    "tailwindcss": "4.1.18",
    "tiny-invariant": "1.3.3",
    "typescript": "5.9.3",
    "unist-util-visit": "5.1.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,md,mdx,astro}": "prettier --write",
    "src/**/*.{js,ts,jsx,tsx,astro}": "eslint --fix"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "github_pages": "https://erkobridee.github.io",
  "engines": {
    "node": ">=20.18.2 <=22.x.x"
  }
}